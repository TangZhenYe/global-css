/*单行文字溢出省略*/
.ellip {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/*多行文字溢出省略*/
.ellip-more {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

