/*hideBox*/
.hide-box {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0,0,0,.8);
  z-index: 99999;
}

/*fixBox*/
.fix-box {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  background-color: #F3F5F7;
  bottom: 0;
  overflow-y: scroll;
}

